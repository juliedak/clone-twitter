<% @user.each do |user| %>
  <%= user.name %>
  <%= user.email %>
<% end %>

<%= link_to "Show",  user_path(@user)%>
<%= link_to "Edit",  edit_user_path %>
<%= link_to "New", new_user_path %>
<%= link_to "Destroy", user_path(@user),  method: :delete,
            data: { confirm: "Are you sure?" }%>
